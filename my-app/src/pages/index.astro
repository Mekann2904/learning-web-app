---
import Base from "../layouts/Base.astro";
import DashboardOverview from "../components/DashboardOverview";
import DashboardShell from "../components/dashboard-shell";
---
<Base title="ダッシュボード">
  <section class="relative space-y-6">
    <DashboardShell client:load />
    <DashboardOverview client:load />
    <img
      src="/images/998891d6317afed3cb16bae5885ac32b.jpg"
      alt="cat"
      aria-hidden
      class="pointer-events-none select-none absolute bottom-4 right-4 w-56 opacity-10 sm:w-72 md:w-80 lg:w-96"
    />
  </section>
</Base>
